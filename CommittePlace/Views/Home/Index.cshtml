@model List<StudentDataViewModel>
<!DOCTYPE html>
<html>
<head>
    <title>Test</title>
    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/css/style.css" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta charset="utf-8" />

</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid" style="overflow:hidden; height:85px;">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="http://www.eelu.edu.eg/"><img class="col-md-offset-3" src="~/imgs/logo.png" style="width:65px;height:65px; border-radius:100%;" /></a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1" style="float:right;">
                <ul class="nav navbar-nav">

                    <li><a href="#" class="h1">المركز الدراسى باسيوط</a></li>

                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>

    <div class="container">
        <div>
            <form asp-action="GetStudentData" asp-controller="StudentData" method="get" class="text-right col-md-11 col-md-offset-1" role="search">
                <div class="alert alert-success col-md-10 text-center" role="alert"><h1> أدخل الرقم الجامعى لمعرفة لجنتك </h1></div>
                <div class="col-md-12">
                    <button type="submit" class="btn btn-default col-xs-4 col-md-2">Submit</button>
                    <div class="form-group col-md-8 col-xs-8">
                        <input name="studentId" class="form-control" placeholder="Search" />
                    </div>
                </div>
            </form>
        </div>

        <div class="col-md-12" style="height:10px; margin-top:45px;"></div>

        <div class="text-right col-md-7 col-md-offset-2">
            <label class="col-md-4 list-group-item list-group-item-success">@Model[0].StudentId</label>
            <label class="col-md-8 list-group-item list-group-item-success">@Model[0].StudentArabicName</label>
            <label class="col-md-4 list-group-item list-group-item-success">@Model[0].StudentCenter</label>
            <label class="col-md-8 list-group-item list-group-item-success">@Model[0].StudentDegree</label>
        </div>

        <div class="col-md-12" style="margin-top:20px;">
            <table class="col-md-12 col-xs-12 text-center">
                <tr>
                    <td class="col-xs-2 col-md-2 list-group-item list-group-item-success"><label>Place</label></td>
                    <th class="col-xs-1 col-md-1 list-group-item list-group-item-success"><label>Room</label></th>
                    <th class="col-xs-4 col-md-4 list-group-item list-group-item-success"><label>Exam Time</label></th>
                    <td class="col-xs-3 col-md-3 list-group-item list-group-item-success"><label>Course Name</label></td>
                </tr>
                @foreach (var studentData in Model)
                {
                    <tr>
                        <td class="col-xs-3 col-md-3 list-group-item list-group-item-success"><label>@studentData.RoomPlace</label></td>

                        <td class="col-xs-4 col-md-4 list-group-item list-group-item-success"><label>@studentData.RoomId</label></td>
                        <td class="col-xs-1 col-md-1 list-group-item list-group-item-success"><label>@studentData.ExamTime</label></td>

                        <td class="col-xs-2 col-md-2 list-group-item list-group-item-success"><label>@studentData.CourseName</label></td>

                    </tr>
                }
            </table>
        </div>

    </div>
    <div id="footerDiv" class="container-fluid" style="height:130px; color:white;background-color:#222222;
margin-top:50px; padding:50px;">
        <label class="h6"> Prepared by Eng. Abd Elrahman Sayed Younis.</label>
    </div>

    <script src="~/js/jquery.js"></script>
    <script src="~/js/bootstrap.js"></script>

</body>
</html>
